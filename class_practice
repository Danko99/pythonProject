import string


class Point:

    def set_coordinates(self, *args):
        alphabet = list(string.ascii_lowercase)
        if 16 > len(args) >= 2:
            for i in range(0, len(args)):
                setattr(self, alphabet[i], args[i])
    def getdistance(self,object):
        dist_dict1=self.__dict__
        dist_dict2 = object.__dict__
        print(dist_dict1, dist_dict2)

p1 = Point()
p2=Point()
p1.set_coordinates(1, 2, 3)
p2.set_coordinates(1, 2, 4)
p2.getdistance(p1)
print(p1.a)
